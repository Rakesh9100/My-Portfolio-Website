{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "preserve",
        "jsxImportSource": "astro",
        "types": ["astro/client", "astro/env"],
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "noEmit": true
    }
}